{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-experiments-dev-useast-adf"
		},
		"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StoragAccountConnString"
		},
		"AzureDatabricks1_properties_typeProperties_domain": {
			"type": "string",
			"defaultValue": "https://adb-2741429623227012.12.azuredatabricks.net"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "@linkedService().ClusterID"
		},
		"azure_key_vault_primary_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().KVUrl}"
		},
		"default_properties_Global-KV-Name_value": {
			"type": "string",
			"defaultValue": "KVadfexperiments-dev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-experiments-dev-useast-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"default_properties_Global-KV-Name_value": {
						"value": "[parameters('default_properties_Global-KV-Name_value')]"
					},
					"azure_key_vault_primary_properties_typeProperties_baseUrl": {
						"value": "[parameters('azure_key_vault_primary_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureDatabricks1_properties_typeProperties_domain": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_domain')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}