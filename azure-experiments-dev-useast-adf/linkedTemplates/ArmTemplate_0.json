						"type": "String"
					},
					{
						"name": "m_dep",
						"type": "String"
					},
					{
						"name": "mobile_wt",
						"type": "String"
					},
					{
						"name": "n_cores",
						"type": "String"
					},
					{
						"name": "pc",
						"type": "String"
					},
					{
						"name": "px_height",
						"type": "String"
					},
					{
						"name": "px_width",
						"type": "String"
					},
					{
						"name": "ram",
						"type": "String"
					},
					{
						"name": "sc_h",
						"type": "String"
					},
					{
						"name": "sc_w",
						"type": "String"
